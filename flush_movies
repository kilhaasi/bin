. /root/bin/.config
cd $IN
counter=0
until [ $counter = 64 ]
do
let counter=$counter+1
 rename " " "_" *
done

mkdir /tmp/flush_movies
clear
echo "============== Video Files ====================="
echo
ls -lh 

for datei in $(ls )
do
 echo $datei
 read zielname
 if [ $zielname != "x" ]
 then
  mv -v $datei /tmp/flush_movies/$zielname
 fi
done

mv -v /tmp/flush_movies/* $MOVIES
